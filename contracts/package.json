{
  "name": "contracts",
  "version": "1.0.0",
  "description": "GRPC contracts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "generate": "protoc -I ./proto ./proto/*.proto --ts_proto_out=./gen --ts_proto_opt=nestJs=true,package=omit"
  },
  "files": [
    "proto",
    "gen",
    "dist"
  ],
  "dependencies": {
    "@nestjs/microservices": "^11.1.11",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "typescript": "^5.9.3"
  }
}
